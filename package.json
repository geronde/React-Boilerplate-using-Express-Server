{
  "name": "reactboilerplate",
  "version": "1.0.0",
  "description": "reactboilerplate for development and production",
  "scripts": {
    "webpack": "webpack",
    "clean": "rimraf ./build/*",
    "dev": "set NODE_ENV='development' & node devServer.js --colors --progress",
    "prod:server": "node prodServer.js",
    "prod:client": "set NODE_ENV='production' & webpack --config prod.config.js --progress --colors ",
    "build": "npm run prod:client & npm run prod:server"
  },
  "author": "L",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.1",
    "postcss-loader": "^2.0.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.20.0"
  },
  "dependencies": {
    "connect-history-api-fallback": "^1.4.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0"
  }
}
